#PBS -lwalltime=2:00:00  
                         # 2 hours wall-clock 
                         # time allowed for this job
#PBS -lnodes=1:ppn=8

                         # 1 node for this job
#PBS -S /bin/bash
echo start of job in directory $PBS_O_WORKDIR
echo number of nodes is $n
echo the allocated nodes are:
cat $PBS_NODEFILE
cd /home/ndrost/gene-sequencing

SERVER_ADDRESS=`cat server_address`
HUB_ADDRESS=`cat hub_address`

for i in `seq 8`  ; do
./bin/dsearch -Dibis.pool.name=genes -Dibis.server.address=$SERVER_ADDRESS -Dibis.hub.addresses=$HUB_ADDRESS testdata/input_ddbjbct14.txt dc&
done

wait

